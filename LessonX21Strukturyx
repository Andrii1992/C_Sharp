using System;

namespace Lesson25Struktury // Структури меньше місьця займають ніж обєкти
{
	struct Book {
		public string title;
		public string name;
		public string author;
	}
	class MainClass
	{
		public static void Main(string[] args)
		{

			Book bo;
			bo.author = "King";

			Console.WriteLine("Hello World!");
		}
	}
}
